{
  "name": "beispiel",
  "version": "1.0.0",
  "description": "Beispiel",
  "main": "dist/index.js",
  "scripts": {
    "test": "cd dist && mocha -c --throw-deprecation --check-leaks -R mochawesome dist\\buchverwaltung\\router\\index.spec.js",
    "build": "gulp default",
    "start": "cd dist && node ../node_modules/nodemon/bin/nodemon.js",
    "mongostart": "mongod --auth --dbpath C:/Zimmermann/MongoDB-data/db --logpath C:/Zimmermann/MongoDB-data/server.log --wiredTigerCacheSizeGB 1 --nssize 8 --maxConns 5 --nojournal",
    "mongoshutdown": "mongo -u admin -p p --eval \"db.shutdownServer()\" admin",
    "mongoimport": "mongoimport -v -u zimmermann -p p -d mydb -c buecher --drop --file dbimport/buecher.json",
    "mongoexport": "mongoexport -v -u zimmermann -p p -d mydb -c buecher -o buecher.json --pretty",
    "mongoexpress": "cd node_modules/mongo-express && node app.js -u zimmermann -p p -d mydb"
  },
  "private": true,
  "homepage": "https://www.hs-karlsruhe.de",
